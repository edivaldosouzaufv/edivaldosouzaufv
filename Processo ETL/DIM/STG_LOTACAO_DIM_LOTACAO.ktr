    <name>STG_Lotacao</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Pesquisa</connection>
    <sql>SELECT lotacaocodigo, 
case when siglaufvestrutura = 'DAA' then 'Departamento de Agronomia'
else lotacaodescricao end as lotacaodescricao, 
(case when siglaufvestrutura = 'PAD' then
	'PAD'
else siglaufvestrutura end)  as siglaufvestrutura,
	case when lotacaodescricao like 'CAF%' then 'CAF' 
	when lotacaodescricao like 'CRP%' then 'CRP' 
	when lotacaodescricao like 'RTR%' then 'RTR' 
	when lotacaodescricao like 'CTM%' then 'CTM' 
	when lotacaodescricao like 'PGP%' then 'PGP' 
	when lotacaodescricao like 'PPG%' then 'PPG' 
	when lotacaodescricao like 'IBA%' then 'RTR'
	when lotacaodescricao like 'PAD%' then 'PAD' 
  	when siglaufvestrutura in ('DDI', 'DAP', 'DDI', 'DDA','GAB','AIN', 'PRJ', 'HOS', 'CEA','EBZ', 'EBH', 'CTA', 'CDC', 'DRC', 'SCA', 'CEA', 'CTA', 'SOC', 'CTV', 'DRI', 'DRC', 'IPP', 'CDC') then 'RTR' 
	when lotacaodescricao like 'DZO%' then 'CCA'
	when lotacaodescricao like 'RTV%' then 'RTR'
	when siglaufvestrutura in ('PCD','DAE', 'DAL', 'DSA', 'DVP', 'SPS') then 'PCD'
	when siglaufvestrutura in (	'PRE','CPV', 'RES', 'PVA', 'PVB', 'COL', 'BBT', 'UAE') then 'PRE'
	when siglaufvestrutura in ('DPA','DFN', 'DTI', 'DMT', 'DGI', 'PPO') then 'PPO'
	when siglaufvestrutura in ('DGT', 'DPT', 'DUR', 'DTP','PAD','DGS', 'DUB', 'DVG','DLS', 'SVG','DAG', 'DPC','DOB','COB','DPP','GEE', 'GEF', 'GPC', 'GTL', 'DTR', 'DMU', 'DPR', 'DPO', 'DOP', 'DME', 'DCO', 'UHI', 'DMC', 'DPJ', 'DIM', 'DIN', 'DLO', 'DLS', 'Sec', 'SEC', 'Par', 'DCI') then 'PAD'
	when siglaufvestrutura in ('PEC','EDT', 'edt','DEV', 'DEX', 'DGU', 'DAC') then 'PEC'
	when siglaufvestrutura in ('CCH','DGE', 'DHI', 'DLA', 'DPD', 'DPE', 'DED', 'DEE', 'DCS', 'DAH', 'DCM', 'DAD') then 'CCH'
	when siglaufvestrutura in ('CCA','DAA', 'DEA', 'DEF', 'DER', 'DFT', 'DFP', 'DPS', 'DZO') then 'CCA'
	when siglaufvestrutura in ('CCB', 'DVT', 'DEM', 'DMB', 'DNS', 'DES', 'DDE', 'DBA', 'DBB', 'DBG', 'DBV') then 'CCB'
	when siglaufvestrutura in ('CCE','DAU', 'DEC', 'DET', 'DEL', 'DMA', 'DEP', 'DEQ', 'DTA', 'DPF', 'DPI') then 'CCE'
	end as siglasuperior,
case 
	when siglaufvestrutura = 'DFT' then 'DAA'
	when siglaufvestrutura = 'DAA' then 'DAA'
	when lotacaodescricao like 'CRP-IAP%' then 'IAP' 
	when lotacaodescricao like 'CRP-IBP%' then 'IBP' 
	when lotacaodescricao like 'CRP-IHP%' then 'IHP' 
	when lotacaodescricao like 'CRP-IEP%' then 'IEP'
	when lotacaodescricao like 'CRP%' then 'CRP'
	when lotacaodescricao like 'CAF%' then siglaufvestrutura 
	when lotacaodescricao like 'RTR%' then 'RTR' 
	when lotacaodescricao like 'PGP%' then 'PGP' 
	when lotacaodescricao like 'PPG%' then 'PPG' 
	when lotacaodescricao like 'PAD%' then 'PAD' 
	when siglaufvestrutura in ('DGT', 'DPT', 'DUR', 'DTP','PAD','DGS', 'DUB', 'DVG','DLS', 'SVG','DAG', 'DPC','DOB','COB','DPP','GEE', 'GEF', 'GPC', 'GTL', 'DTR', 'DMU', 'DPR', 'DPO', 'DOP', 'DME', 'DCO', 'UHI', 'DMC', 'DPJ', 'DIM', 'DIN', 'DLO', 'DLS', 'Sec', 'SEC', 'Par', 'DCI') then 'PAD'
	when siglaufvestrutura = 'DAP' then 'DRC'
	when siglaufvestrutura = 'DTR' then 'DLO'
	when siglaufvestrutura = 'SOC' then 'SOC'
    when siglaufvestrutura = 'GAB' then 'RTR'
	when siglaufvestrutura = 'DDA' then 'DRC'
	when siglaufvestrutura = 'EBH' then 'RTR'
	when siglaufvestrutura = 'CTV' then 'RTR'
	when siglaufvestrutura = 'DFT' then 'DAA'
	when siglaufvestrutura = 'SPS' then 'PCD'
	when siglaufvestrutura = 'DDI' then 'DRC'
	when siglaufvestrutura = 'DAG' then 'DMU'
	when siglaufvestrutura = 'DVG' then 'DLS'
	when siglaufvestrutura = 'CBO' then 'DLS'
	when siglaufvestrutura = 'DGS' then 'DMU'
	when siglaufvestrutura = 'DCI' then 'DIM'
	when siglaufvestrutura = 'DME' then 'DIM'
	when siglaufvestrutura = 'PPO' then siglaufvestrutura
	else siglaufvestrutura
end as subsiglasuperior
,campuscodigo

FROM stg_lotacao where lotacaoextinta = 'N';</sql>
