        <jsScript_script>//Script here


var distancia = distance /1000;
var duracao = parseInt(duration);


//var duracao = parseFloat(duracao.toFixed(2));
var distancia = parseFloat(distancia.toFixed(1));
distancia = distancia+' km';
var duracao = segParaHora(duration, with_seg = true);


function segParaHora(duration){
    
  var hours = Math.round( duration / 3600 );
  var minutes = Math.round( (duration % 3600) / 60 );
  var seconds = duration % 60;
    
  minutes = minutes &lt; 10 ? '0' + minutes : minutes;      
  seconds = seconds &lt; 10 ? '0' + seconds : seconds;
  hours = hours &lt; 10 ? '0' + hours : hours;
    
  return  hours + ":" + minutes;
}


</jsScript_script>
